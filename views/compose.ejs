<%- include("partials/header") -%>

<h1>Compose</h1>
<form class="composeItem" action="/compose" method="post">
    <div class="form-group">
        <label for="composeTitleid" class="form-label">Title</label>
        <input class="form-control" type="text" name="composeTitle" id="composeTitleid" autocomplete="off">
        <label for="composePostid" class="form-label">Post</label>
        <textarea class="form-control" type="text" name="composeText" id="composePostid" autocomplete="off"
            rows="5"></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="button">Publish</button>
</form>

<%- include("partials/footer") -%>